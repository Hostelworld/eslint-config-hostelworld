{
    "env": {
        "browser": true,
        "jest": true
    },
    "plugins": [
        "html"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "globals": {
        "$": true,
        "jQuery": true,
        "google": true,
        "googletag": true,
        "_": true,
        "WRI": true,
        "HwReg": true,
        "s": true,
        "translate": true,
        "wNumb": true,
        "Set": true,
        "moment": true,
        "Promise": true,
        "_gaq": true,
        "process": true
    },
    "extends": "standard",
    "rules": {
        "indent": [
            "error",
            4,
            { "SwitchCase": 1 }
        ],
        "semi": [
            "error",
            "always"
        ],
        "curly": [
            "error",
            "all"
        ]
    }
}
